{
  "JobSiteName": "KellyServices",
  "SourceURL": "https://kelly.secure.force.com/CandidateExperience/CandExpJobSearch?advanced=false&jobCategoryList=null&location=***LOCATION***&radius=60",
  "BaseURL": "https://kelly.secure.force.com",
  "AdditionalFlags" : [ "C__JOB_USE_SELENIUM"],
  "CountryCodes" : ["US"],
  "PluginExtendsClassName" : "AbstractKellyServices",
  "LocationType": "location-city-comma-state",
  "Collections": [
    {
      "Name": "PageFields",
      "Fields": [
        {
          "Name": "NoPostsFound",
          "Selector": "div.jobcount",
          "Index" : 0,
          "Callback" : "matchesNoResultsPattern",
          "CallbackParameter": "0 Results",
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "TotalPostCount",
          "Selector": "div.jobcount",
          "Index" : 0,
          "Pattern": "/([0-9,]+)\\s*Results/",
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "JobPostItem",
          "Selector": "span div.row div.rowheader",
          "Attribute": "collection",
          "Type": "CSS"
        }
      ]
    },
    {
      "Name": "ItemFields",
      "Fields": [
        {
          "Name": "Title",
          "Selector": "div.job",
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "Location",
          "Selector": "div.loc",
          "Index": 0,
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "Category",
          "Selector": "div.type",
          "Index": 0,
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "Url",
          "Selector": "div.view a",
          "Attribute": "href",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "JobSitePostId",
          "Selector": "div.share",
          "Attribute" : "id",
          "Index": 0,
          "Type": "CSS"
        },

        {
          "Name": "PostedAt",
          "Selector": "div.date",
          "Index": 0,
          "Attribute": "text",
          "Type": "CSS"
        }
      ]
    }
  ],
  "Pagination": {
    "Type": "PAGE-CALLBACK",
    "PageLimit": 50
  }
}