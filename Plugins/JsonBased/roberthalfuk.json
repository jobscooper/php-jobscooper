{
  "JobSiteName": "RobertHalfUK",
  "BaseURL": "https://www.roberthalf.co.uk/",
  "SourceURL": "https://www.roberthalf.co.uk/jobs/all-jobs/***LOCATION***?pageSize=1500",
  "LocationType": "location-city",
  "CountryCodes" : ["GB"],
  "AdditionalFlags" : ["JOB_KEYWORD_PARAMETER_SPACES_AS_DASHES"],
  "Collections": [
    {
      "Name": "PageFields",
      "Fields": [
        {
          "Name": "TotalPostCount",
          "Selector": "div.job-search-result-counter",
          "Pattern": "/.*?of\\s(\\d+)\\s*/",
          "Callback": "combineTextAllChildren",
          "Index": 0,
          "Attribute": "node",
          "Type": "CSS"
        },
        {
          "Name": "JobPostItem",
          "Selector": "table.job-search-results tr",
          "Attribute": "collection",
          "Type": "CSS"
        }
      ]
    },
    {
      "Name": "ItemFields",
      "Fields": [
        {
          "Name": "Title",
          "Selector": "td.JobTitle a",
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "Url",
          "Selector": "td.JobTitle a",
          "Attribute": "href",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "Location",
          "Selector": "span.midRight",
          "Index" : 0,
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "Category",
          "Selector": "span.midLeft",
          "Index" : 0,
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "JobSitePostId",
          "Pattern": "/\/job\/([^?]+)/i",
          "Type": "SOURCEFIELD",
          "Field": "Url"
        },
        {
          "Name": "PostedAt",
          "Selector": "span.topRight",
          "Index" : 0,
          "Attribute": "text",
          "Type": "CSS"
        }
      ]
    }
  ],
  "Pagination": {
    "Type": "NEXT-BUTTON",
    "Selector": "a.rh-pager__link--arrow",
    "PageLimit": 1500
  }
}