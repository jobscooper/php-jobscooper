{
  "AgentName": "Startwire",
  "SourceURL": "https://www.startwire.com/search",
  "Collections": [
    {
      "Name": "PageFields",
      "Fields": [
      {
          "Name": "JobPostItem",
          "Selector": ".js-job",
          "Extract": "HTML",
          "Attribute": "node",
          "Type": "CSS"
        },
        {
          "Name": "TotalPostCount",
          "Selector": "div.count-jobs div.number",
          "Extract": "TEXT",
          "Type": "CSS"
        },
        {
          "Name": "pages_count",
          "Selector": "div.js-infinite-scroll",
          "Extract": "ATTR",
          "Attribute": "data-total-pages",
          "Type": "CSS"
        }
      ]
    },
    {
      "Name": "ItemFields",
      "Fields": [
        {
          "Name": "Title",
          "Selector": ".title",
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "Location",
          "Selector": "span.location",
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "Company",
          "Selector": ".company-name",
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "company_logo",
          "Selector": "img",
          "Extract": "ATTR",
          "Attribute": "href",
          "Type": "CSS"
        },
        {
          "Name": "Url",
          "Selector": "a[class*='link-job']",
          "Extract": "ATTR",
          "Attribute": "href",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "JobSitePostId",
          "Selector": "div.js-job",
          "Extract": "ATTR",
          "Attribute": "data-hash-uniq",
          "Index": 0
        },
        {
          "Name": "PostedAt",
          "Selector": "div.posted-date span",
          "Extract": "HTML",
          "Attribute": null,
          "Type": "CSS"
        }
      ]
    }
  ],
  "Pagination": {
    "Type": "INFINITE-SCROLL-NO-CONTROL",
    "PageLimit" : 40
  }
}