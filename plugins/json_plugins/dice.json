{
  "AgentName": "Dice",
  "BaseURL": "http://www.dice.com/",
  "SourceURL": "https://www.dice.com/jobs/advancedResult.html?for_one=&for_all=***KEYWORDS***&for_exact=&for_none=&for_jt=&for_com=&for_loc=***LOCATION***&sort=date&limit=100&radius=50",
  "LocationType": "location-city-comma-statecode",
  "CountryCodes" : ["US"],
  "AdditionalFlags" : ["C__JOB_KEYWORD_SUPPORTS_QUOTED_KEYWORDS", "C__JOB_RESULTS_SHOWN_IN_DATE_DESCENDING_ORDER"],
  "PluginExtendsClassName" : "AbstractDice",
  "Collections": [
    {
      "Name": "PageFields",
      "Fields": [
        {
          "Name": "NoPostsFound",
          "Selector": "h1.h1",
          "Extract": "TEXT",
          "Index" : 0,
          "Callback" : "matchesNoResultsPattern",
          "CallbackParameter": "No jobs found",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "TotalPostCount",
          "Selector": "span#posiCountId",
          "Extract": "TEXT",
          "Type": "CSS"
        },
        {
          "Name": "JobPostItem",
          "Selector": "div.complete-serp-result-div",
          "Extract": "CSS",
          "Attribute": "collection",
          "Type": "CSS"
        }
      ]
    },
    {
      "Name": "ItemFields",
      "Fields": [
        {
          "Name": "Title",
          "Selector": "h3 a.dice-btn-link",
          "Extract": "ATTR",
          "Attribute": "title",
          "Type": "CSS"
        },
        {
          "Name": "Url",
          "Selector": "h3 a.dice-btn-link",
          "Extract": "ATTR",
          "Attribute": "href",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "JobSitePostId",
          "Selector": "h3 a.dice-btn-link",
          "Extract": "ATTR",
          "Attribute": "value"
        },
        {
          "Name": "Location",
          "Selector": "span.jobLoc",
          "Index" : 0,
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "Company",
          "Selector": "span.compName",
          "Index" : 0,
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "PostedAt",
          "Selector": "span[itemprop=datePosted]",
          "Index" : 0,
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        }
      ]
    }
  ],
  "Pagination": {
    "PageLimit": 100,
    "Type": "PAGE-CALLBACK",
    "PageDelaySeconds" : 5
  }
}