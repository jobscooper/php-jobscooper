{
  "JobSiteName": "TotalJobs",
  "SourceURL": "https://www.totaljobs.com/jobs/***KEYWORDS***/in-***LOCATION:{Place}***?radius=20&postedwithin=3&Sort=2&page=***PAGE_NUMBER***",
  "CountryCodes" : ["UK"],
  "AdditionalFlags" : ["C__JOB_KEYWORD_PARAMETER_SPACES_AS_DASHES"],
  "Collections": [
    {
      "Name": "PageFields",
      "Fields": [
        {
          "Name": "TotalPostCount",
          "Selector": "div.page-title span",
          "Extract": "TEXT",
          "Type": "CSS"
        },
        {
          "Name": "JobPostItem",
          "Selector": "div.job",
          "Extract": "CSS",
          "Attribute": "collection",
          "Type": "CSS"
        }
      ]
    },
    {
      "Name": "ItemFields",
      "Fields": [
        {
          "Name": "Title",
          "Selector": "div.job-title a h2",
          "Extract": "TEXT",
          "Type": "CSS"
        },
        {
          "Name": "Url",
          "Selector": "div.job-title a",
          "Extract": "ATTR",
          "Attribute": "href",
          "Type": "CSS"
        },
        {
          "Name": "JobSitePostId",
          "Selector": "button[data-job-id]",
          "Extract": "ATTR",
          "Attribute": "data-job-id"
        },
        {
          "Name": "Location",
          "Selector": "li.location",
          "Extract": "TEXT",
          "Attribute": null,
          "Index" : 0,
          "Type": "CSS"
        },
        {
          "Name": "Company",
          "Selector": "li.company h3 a",
          "Extract": "TEXT",
          "Attribute": null,
          "Index" : 0,
          "Type": "CSS"
        },
        {
          "Name": "EmploymentType",
          "Selector": "li.job-type span",
          "Extract": "TEXT",
          "Attribute": null,
          "Index" : 0,
          "Type": "CSS"
        },
        {
          "Name": "PostedAt",
          "Selector": "li.date-posted span",
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        }
      ]
    }
  ],
  "Pagination": {
    "PageLimit": 20,
    "Type": "PAGE-VIA-URL"
  }
}