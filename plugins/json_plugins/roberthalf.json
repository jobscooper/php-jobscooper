{
  "JobSiteName": "RobertHalf",
  "BaseURL": "https://www.roberthalf.com/",
  "SourceURL": "https://www.roberthalf.com/technology/jobs/all-jobs/***LOCATION***?pageSize=1500",
  "LocationType": "location-city-dash-statecode",
  "CountryCodes" : ["US"],
  "AdditionalFlags" : ["C__JOB_KEYWORD_PARAMETER_SPACES_AS_DASHES"],
  "Collections": [
    {
      "Name": "PageFields",
      "Fields": [
        {
          "Name": "TotalPostCount",
          "Selector": "div.rh-job-result-table__counter label",
          "Index" : 1,
          "Extract": "REGEX",
          "Pattern": "/of\\s*(\\d+)/",
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "JobPostItem",
          "Selector": "div.row .rh-job-result-table__row",
          "Extract": "CSS",
          "Attribute": "collection",
          "Type": "CSS"
        }
      ]
    },
    {
      "Name": "ItemFields",
      "Fields": [
        {
          "Name": "Title",
          "Selector": "a.rh-job-result-table__job-title",
          "Extract": "TEXT",
          "Attribute": "text",
          "Type": "CSS"
        },
        {
          "Name": "Url",
          "Selector": "a.rh-job-result-table__job-title",
          "Extract": "ATTR",
          "Attribute": "href",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "Location",
          "Selector": "span.rh-job-result-table__location",
          "Index" : 1,
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "Category",
          "Selector": "span.rh-job-result-table__emptype",
          "Index" : 1,
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "JobSitePostId",
          "Pattern": "/\\/job\\/([^?]+)/i",
          "Extract": "REGEX",
          "Type": "REGEX",
          "Field": "Url"
        },
        {
          "Name": "PostedAt",
          "Selector": "span.rh-job-result-table__postdate",
          "Index" : 1,
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        }
      ]
    }
  ],
  "Pagination": {
    "Type": "NEXT-BUTTON",
    "Selector": "a.rh-pager__link--arrow",
    "PageLimit": 1500
  }
}