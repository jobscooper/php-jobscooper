{
  "JobSiteName": "LucasGroup",
  "SourceURL": "https://www.lucasgroup.com/executive-jobs/job-search/job-search-results",
  "BaseURL": "https://www.lucasgroup.com",
  "AdditionalFlags" : [ "C__JOB_PAGECOUNT_NOTAPPLICABLE__", "C__JOB_ITEMCOUNT_NOTAPPLICABLE__", "C__JOB_USE_SELENIUM__"],
  "Collections": [
    {
      "Name": "PageFields",
      "Fields": [
      {
          "Name": "JobPostItem",
          "Selector": "div.jobsearch-results div.grid",
          "Extract": "HTML",
          "Attribute": "node",
          "Type": "CSS"
        }
      ]
    },
    {
      "Name": "ItemFields",
      "Fields": [
        {
          "Name": "Title",
          "Selector": "h3",
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "Location",
          "Selector": "div.jsr-pg",
          "Extract": "TEXT",
          "Index": 1,
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "Category",
          "Selector": "div.jsr-pg",
          "Extract": "TEXT",
          "Index": 0,
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "Url",
          "Selector": "a",
          "Extract": "ATTR",
          "Attribute": "href",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "JobSitePostId",
          "Selector": "a",
          "Extract": "REGEX",
          "Type": "CSS",
          "Attribute": "href",
          "Pattern" : "/[Jj]ob[Ii][dD]=(\\w+)/"
        }
      ]
    }
  ],
  "Pagination": {
    "Type": "NONE"  }
}