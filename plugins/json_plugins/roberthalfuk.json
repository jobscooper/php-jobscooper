{
  "AgentName": "RobertHalfUK",
  "BaseURL": "https://www.roberthalf.co.uk/",
  "SourceURL": "https://www.roberthalf.co.uk/jobs/all-jobs/***LOCATION***?pageSize=1500",
  "LocationType": "location-city",
  "CountryCodes" : ["UK"],
  "AdditionalFlags" : ["JOB_KEYWORD_PARAMETER_SPACES_AS_DASHES"],
  "Collections": [
    {
      "Name": "PageFields",
      "Fields": [
        {
          "Name": "TotalPostCount",
          "Selector": "div.job-search-result-counter",
          "Extract": "REGEX",
          "Pattern": "/.*?(\\d+)\\s*/",
          "Callback": "combineTextAllNodes",
          "Index": 0,
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "JobPostItem",
          "Selector": "table.job-search-results tr",
          "Extract": "CSS",
          "Attribute": "collection",
          "Type": "CSS"
        }
      ]
    },
    {
      "Name": "ItemFields",
      "Fields": [
        {
          "Name": "Title",
          "Selector": "td.JobTitle a",
          "Extract": "TEXT",
          "Attribute": "plaintext",
          "Type": "CSS"
        },
        {
          "Name": "Url",
          "Selector": "td.JobTitle a",
          "Extract": "ATTR",
          "Attribute": "href",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "LocationFromSource",
          "Selector": "span.midRight",
          "Index" : 0,
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "Category",
          "Selector": "span.midLeft",
          "Index" : 0,
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "JobSitePostId",
          "Pattern": "/\/job\/([^?]+)/i",
          "Extract": "REGEX",
          "Type": "REGEX",
          "Field": "job_post_url"
        },
        {
          "Name": "PostedAt",
          "Selector": "span.topRight",
          "Index" : 0,
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        }
      ]
    }
  ],
  "Pagination": {
    "Type": "next-button",
    "Selector": "a.rh-pager__link--arrow",
    "PageLimit": 1500
  }
}