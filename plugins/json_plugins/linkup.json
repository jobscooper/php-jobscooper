{
  "AgentName": "LinkUp",
  "SourceURL": "https://www.linkup.com/search/results/***KEYWORDS***-jobs?location=***LOCATION***&pageNum=***PAGE_NUMBER***&sort=d",
  "LocationType": "location-city-comma-state",
  "Collections": [
    {
      "Name": "PageFields",
      "Fields": [
        {
          "Name": "tag_listings_noresults",
          "Selector": "div.search-details p span",
          "Extract": "TEXT",
          "Index" : 0,
          "Callback" : "matchesNoResultsPattern",
          "CallBackParameter": "0 of 0",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "tag_listings_count",
          "Selector": "div.search-details p span",
          "Index" : 0,
          "Extract": "REGEX",
          "Pattern": "/\\s*of\\s*([\\d,]+)\\s*/",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "tag_listings_section",
          "Selector": "div.row .job-listing",
          "Extract": "CSS",
          "Attribute": "collection",
          "Type": "CSS"
        }
      ]
    },
    {
      "Name": "ItemFields",
      "Fields": [
        {
          "Name": "tag_title",
          "Selector": "a.search-result-link",
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "tag_link",
          "Selector": "a.search-result-link",
          "Extract": "ATTR",
          "Attribute": "href",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "tag_job_id",
          "Pattern": "/.*?\/(.*?)/",
          "Extract": "REGEX",
          "Type": "REGEX",
          "Field": "job_post_url"
        },
        {
          "Name": "tag_location",
          "Selector": "div.show-on-large .hide-on-med-and-down p span",
          "Index" : 0,
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "tag_company",
          "Selector": "div.show-on-large .hide-on-med-and-down p span",
          "Index" : 1,
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        },

        {
          "Name": "tag_job_posting_date",
          "Selector": "div.show-on-large .hide-on-med-and-down p span",
          "Index" : 2,
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        }
      ]
    }
  ],
  "Pagination": {
    "Type": "PAGE-VIA-URL",
    "PageLimit": 25
  }
}