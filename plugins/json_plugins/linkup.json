{
  "AgentName": "LinkUp",
  "SourceURL": "https://linkup.com/search/results?keyword=***KEYWORDS***&location=***LOCATION***&distance=25&posted=1d&sort=d&pageNum=***PAGE_NUMBER***",
  "LocationType": "location-city-comma-state",
  "Collections": [
    {
      "Name": "PageFields",
      "Fields": [
        {
          "Name": "NoPostsFound",
          "Selector": "div.search-details p span",
          "Extract": "TEXT",
          "Index" : 0,
          "Callback" : "matchesNoResultsPattern",
          "CallbackParameter": "0 of 0",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "TotalPostCount",
          "Selector": "div.search-details div span.vertical-bar",
          "Index" : 0,
          "Extract": "REGEX",
          "Pattern": "/.*?\\s([0-9,]+)\\sJobs/",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "JobPostItem",
          "Selector": "div.row .job-listing",
          "Extract": "CSS",
          "Attribute": "collection",
          "Type": "CSS"
        }
      ]
    },
    {
      "Name": "ItemFields",
      "Fields": [
        {
          "Name": "Title",
          "Selector": "a.search-result-link",
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "Url",
          "Selector": "a.search-result-link",
          "Extract": "ATTR",
          "Attribute": "href",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "JobSitePostId",
          "Pattern": "/.*?\/(.*?)/",
          "Extract": "REGEX",
          "Type": "REGEX",
          "Field": "job_post_url"
        },
        {
          "Name": "Location",
          "Selector": "div.show-on-large .hide-on-med-and-down p span",
          "Index" : 0,
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "Company",
          "Selector": "div.show-on-large .hide-on-med-and-down p span",
          "Index" : 1,
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        },

        {
          "Name": "PostedAt",
          "Selector": "div.show-on-large .hide-on-med-and-down p span",
          "Index" : 2,
          "Extract": "TEXT",
          "Attribute": null,
          "Type": "CSS"
        }
      ]
    }
  ],
  "Pagination": {
    "Type": "PAGE-VIA-URL",
    "PageLimit": 25
  }
}