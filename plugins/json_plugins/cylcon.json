{
  "AgentName" : "Cylcon",
  "BaseURL" : "http://www.cylcon.com/",
  "SourceURL" : "http://www.cylcon.com/jobs.php?q=***KEYWORDS***&l=***LOCATION***&sort=date&radius=50&start=***ITEM_NUMBER***",
  "LocationType" : "location-city-comma-statecode",
  "CountryCodes" : ["US", "UK"],
  "AdditionalFlags" : ["C__JOB_ITEMCOUNT_STARTSATZERO__"],
  "Collections": [
    {
      "Name": "PageFields",
      "Fields": [
        {
          "Name": "TotalPostCount",
          "Selector": "div#searchCount",
          "Extract": "REGEX",
          "Pattern": "/.*?of\\s+(\\d+)/",
          "Attribute": null,
          "Type": "CSS"
        },
        {
          "Name": "JobPostItem",
          "Selector": "div.joblists div.col-lg-10",
          "Extract": "CSS",
          "Attribute": "collection",
          "Type": "CSS"
        }
      ]
    },
    {
      "Name": "ItemFields",
      "Fields": [
        {
          "Name": "Title",
          "Selector": "a",
          "Extract": "TEXT",
          "Attribute": "plaintext",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "Url",
          "Selector": "a",
          "Extract": "ATTR",
          "Attribute": "href",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "JobSitePostId",
          "Pattern": "/.*?[Rr]edirect[Ww][Ee][Bb]\\.php\\?q=(.*)/",
          "Extract": "REGEX",
          "Type": "REGEX",
          "Field": "job_post_url"
        },

        {
          "Name": "Company",
          "Selector": "p strong",
          "Extract": "TEXT",
          "Pattern": "/(.*?)[\\-]{1}.*/",
          "Index": 0,
          "Type": "CSS"
        },
        {
          "Name": "Location",
          "Selector": "p strong span.location",
          "Pattern": "/.*?[\\-]{1}(.*)/",
          "Extract": "TEXT",
          "Type": "CSS"
        },
        {
          "Name": "PostedAt",
          "Selector": "span.date",
          "Index" : 0,
          "Extract": "TEXT",
          "Type": "CSS"
        }
      ]
    }
  ],
  "Pagination": {
    "Type": "PAGE-VIA-URL",
    "PageLimit": 20
  }
}